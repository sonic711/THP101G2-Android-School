<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".TLoginMainFragment">


    <Button
            android:text="假裝會員登入成功"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="158dp" android:layout_marginEnd="159dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="328dp"/>
    <Button
            android:text="假裝廠商登入成功"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2"
            android:layout_marginTop="18dp"
            app:layout_constraintTop_toBottomOf="@+id/button" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="157dp" android:layout_marginEnd="160dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="假裝後台登入成功"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="159dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/button2"
            android:layout_marginEnd="158dp" app:layout_constraintEnd_toEndOf="parent"/>


</androidx.constraintlayout.widget.ConstraintLayout>